{
    "env": {
        "browser": true,
        "es2020": true,
        "jasmine": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:fsd/all",
        "airbnb-base/legacy"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "fsd",
        "jasmine"
    ],
    "rules": {
        "linebreak-style": 0
    }
}
